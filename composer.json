{
    "name": "hanaboso/user-bundle",
    "description": "Hanaboso - user-bundle",
    "version": "1.5.1",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.0",
        "hanaboso/commons-bundle": "^2.3",
        "hanaboso/email-service-bundle": "^1.2",
        "hanaboso/rabbit-mq-bundle": "^1.5",
        "hanaboso/rest-bundle": "^1.1",
        "lcobucci/jwt": "^4.1"
    },
    "require-dev": {
        "ext-apcu": "*",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/mongodb-odm-bundle": "^4.3",
        "doctrine/orm": "^2.9",
        "hanaboso/php-check-utils": "^1.2",
        "symfony/monolog-bundle": "^v3.7",
        "symfony/security-bundle": "^v5.2"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "Install me, if you using MySQL.",
        "doctrine/mongodb-odm-bundle": "Install me, if you using MongoDB."
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\UserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserBundleTests\\": "tests/"
        }
    }
}
