{
    "name": "hanaboso/user-bundle",
    "description": "Hanaboso - user-bundle",
    "license": "Apache-2.0",
    "version": "1.11.0",
    "require": {
        "php": "^8.4",
        "hanaboso/commons-bundle": "^2.12",
        "hanaboso/email-service-bundle": "^1.6",
        "hanaboso/rest-bundle": "^1.5",
        "symfony/yaml": "^7.3",
        "web-token/jwt-library": "^4.0"
    },
    "require-dev": {
        "ext-apcu": "*",
        "doctrine/doctrine-bundle": "^2.15",
        "doctrine/mongodb-odm-bundle": "^5.3",
        "doctrine/orm": "^2.20",
        "hanaboso/php-check-utils": "^1.7",
        "symfony/monolog-bundle": "^3.10",
        "symfony/security-bundle": "^7.3"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "Install me, if you using MySQL.",
        "doctrine/mongodb-odm-bundle": "Install me, if you using MongoDB."
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\UserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserBundleTests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
