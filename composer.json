{
    "name": "hanaboso/user-bundle",
    "description": "Hanaboso - user-bundle",
    "license": "Apache-2.0",
    "version": "1.6.3",
    "require": {
        "php": "^8.1",
        "hanaboso/commons-bundle": "^2.7",
        "hanaboso/email-service-bundle": "^1.2",
        "hanaboso/rest-bundle": "^1.1",
        "symfony/yaml": "^v6.2",
        "web-token/jwt-checker": "^3.1",
        "web-token/jwt-core": "^3.1",
        "web-token/jwt-key-mgmt": "^3.1",
        "web-token/jwt-signature": "^3.1",
        "web-token/jwt-signature-algorithm-hmac": "^3.1"
    },
    "require-dev": {
        "ext-apcu": "*",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/mongodb-odm-bundle": "^4.5",
        "doctrine/orm": "^2.14",
        "hanaboso/php-check-utils": "^1.4",
        "symfony/monolog-bundle": "^v3.8",
        "symfony/security-bundle": "^v6.2"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "Install me, if you using MySQL.",
        "doctrine/mongodb-odm-bundle": "Install me, if you using MongoDB."
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\UserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserBundleTests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
