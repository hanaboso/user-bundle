{
    "name": "hanaboso/user-bundle",
    "description": "Hanaboso - user-bundle",
    "license": "Apache-2.0",
    "version": "1.12.0",
    "require": {
        "php": "^8.4",
        "hanaboso/commons-bundle": "^3.0",
        "hanaboso/email-service-bundle": "^1.7",
        "hanaboso/rest-bundle": "^1.6",
        "symfony/yaml": "^8.0",
        "web-token/jwt-library": "^4.1"
    },
    "require-dev": {
        "ext-apcu": "*",
        "doctrine/doctrine-bundle": "^3.2",
        "doctrine/mongodb-odm-bundle": "^5.5",
        "doctrine/orm": "^3.6",
        "hanaboso/php-check-utils": "^2.0",
        "symfony/monolog-bundle": "^4.0",
        "symfony/security-bundle": "^8.0"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "Install me, if you using MySQL.",
        "doctrine/mongodb-odm-bundle": "Install me, if you using MongoDB."
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\UserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserBundleTests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
